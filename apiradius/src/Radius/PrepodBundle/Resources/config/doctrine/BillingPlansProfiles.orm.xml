<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Radius\PrepodBundle\Entity\BillingPlansProfiles" table="billing_plans_profiles">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="planName" type="string" column="plan_name" length="128" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">the name of the plan</option>
      </options>
    </field>
    <field name="profileName" type="string" column="profile_name" length="256" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">the profile/group name</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
