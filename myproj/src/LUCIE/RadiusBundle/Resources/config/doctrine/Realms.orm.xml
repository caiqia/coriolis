<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LUCIE\RadiusBundle\Entity\Realms" table="realms">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="realmname" type="string" column="realmname" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="authhost" type="string" column="authhost" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="accthost" type="string" column="accthost" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="secret" type="string" column="secret" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ldflag" type="string" column="ldflag" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nostrip" type="integer" column="nostrip" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hints" type="integer" column="hints" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="notrealm" type="integer" column="notrealm" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="creationdate" type="datetime" column="creationdate" nullable="true"/>
    <field name="creationby" type="string" column="creationby" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updatedate" type="datetime" column="updatedate" nullable="true"/>
    <field name="updateby" type="string" column="updateby" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
