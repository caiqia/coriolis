<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LUCIE\RadiusBundle\Entity\Userinfo" table="userinfo">
    <indexes>
      <index name="username" columns="username"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstname" type="string" column="firstname" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastname" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="department" type="string" column="department" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="company" type="string" column="company" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="workphone" type="string" column="workphone" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="homephone" type="string" column="homephone" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mobilephone" type="string" column="mobilephone" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="state" type="string" column="state" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zip" type="string" column="zip" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="notes" type="string" column="notes" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="changeuserinfo" type="string" column="changeuserinfo" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="portalloginpassword" type="string" column="portalloginpassword" length="128" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="enableportallogin" type="integer" column="enableportallogin" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="creationdate" type="datetime" column="creationdate" nullable="true">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="creationby" type="string" column="creationby" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updatedate" type="datetime" column="updatedate" nullable="true">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="updateby" type="string" column="updateby" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
