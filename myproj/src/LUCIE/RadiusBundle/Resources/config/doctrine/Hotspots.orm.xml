<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LUCIE\RadiusBundle\Entity\Hotspots" table="hotspots">
    <indexes>
      <index name="name" columns="name"/>
      <index name="mac" columns="mac"/>
    </indexes>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mac" type="string" column="mac" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="geocode" type="string" column="geocode" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="owner" type="string" column="owner" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailOwner" type="string" column="email_owner" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="manager" type="string" column="manager" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailManager" type="string" column="email_manager" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="company" type="string" column="company" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone1" type="string" column="phone1" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone2" type="string" column="phone2" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="companywebsite" type="string" column="companywebsite" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="companyemail" type="string" column="companyemail" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="companycontact" type="string" column="companycontact" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="companyphone" type="string" column="companyphone" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creationdate" type="datetime" column="creationdate" nullable="true">
      <options>
        <option name="default">1999-10-10 10:10:10</option>
      </options>
    </field>
    <field name="creationby" type="string" column="creationby" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updatedate" type="datetime" column="updatedate" nullable="true">
      <options>
        <option name="default">1999-10-10 10:10:10</option>
      </options>
    </field>
    <field name="updateby" type="string" column="updateby" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
