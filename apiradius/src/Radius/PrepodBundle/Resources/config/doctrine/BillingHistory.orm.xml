<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Radius\PrepodBundle\Entity\BillingHistory" table="billing_history">
    <indexes>
      <index name="username" columns="username"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="planid" type="integer" column="planId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="billamount" type="string" column="billAmount" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="billaction" type="string" column="billAction" length="128" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">Unavailable</option>
      </options>
    </field>
    <field name="billperformer" type="string" column="billPerformer" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="billreason" type="string" column="billReason" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="paymentmethod" type="string" column="paymentmethod" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cash" type="string" column="cash" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creditcardname" type="string" column="creditcardname" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creditcardnumber" type="string" column="creditcardnumber" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creditcardverification" type="string" column="creditcardverification" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creditcardtype" type="string" column="creditcardtype" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creditcardexp" type="string" column="creditcardexp" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="coupon" type="string" column="coupon" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="discount" type="string" column="discount" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="notes" type="string" column="notes" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creationdate" type="datetime" column="creationdate" nullable="true">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="creationby" type="string" column="creationby" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updatedate" type="datetime" column="updatedate" nullable="true">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="updateby" type="string" column="updateby" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
