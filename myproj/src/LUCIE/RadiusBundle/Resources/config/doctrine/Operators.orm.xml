      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstname" type="string" column="firstname" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastname" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="department" type="string" column="department" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="company" type="string" column="company" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone1" type="string" column="phone1" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone2" type="string" column="phone2" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email1" type="string" column="email1" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email2" type="string" column="email2" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="messenger1" type="string" column="messenger1" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="messenger2" type="string" column="messenger2" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="notes" type="string" column="notes" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastlogin" type="datetime" column="lastlogin" nullable="true">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="creationdate" type="datetime" column="creationdate" nullable="true">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="creationby" type="string" column="creationby" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updatedate" type="datetime" column="updatedate" nullable="true">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="updateby" type="string" column="updateby" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
