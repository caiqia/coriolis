{% extends "RadiusPrepodBundle::layout.html.twig" %}
{% trans_default_domain 'RadiusPrepodBundle' %}


{% block title %}{{'voicemail.new.title'|trans}}{% endblock %}

{% block content %}
    <h1>{{'voicemail.new.title'|trans}}</h1>
    {%  if (form is not null) %}
    <form action="{{ url('api_1_post_voicemail') }}" method="POST" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <div>
        <button type="submit" class="btn btn-default">{{'voicemail.new.submit'|trans}}</button>
        </div>

    </form>
    {% endif %}
{% endblock %}
